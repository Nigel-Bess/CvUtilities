<Window x:Class="CvBuilder.Ui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CvBuilder.Ui"
       
        xmlns:deploy="clr-namespace:CvBuilder.Ui.Deploy"
        mc:Ignorable="d"
        Title="CV Utilities" Height="450" Width="800"
        d:DataContext="{d:DesignInstance Type=local:MainWindowViewModel}">
    <Grid>
        <TabControl>
            <TabItem Header="Build and Deploy">
                <deploy:BuildAndDeployControl DataContext="{Binding BuildAndDeployVm}"/>
             </TabItem>
        </TabControl>
    </Grid>
</Window>
